<section class="family-registration hidden">

	<form enctype="multipart/form-data" action="/register" class="registration-form form--family-registration" method="POST">
		<input type="hidden" class="registration-type" name="registrationType" value="family">
		{{#if redirectUrl}}
			<input type="hidden" name="redirectUrl" value="{{ redirectUrl }}">
		{{/if}}

		<section class="family-account-setup-section">
			<div class="form-section-header">Account Setup</div>

			<span class="required required-description">* Required</span>

			<div class="row form-group">
				<div class="col-sm-12">
					<input type="email" id="family-email" class="input" name="email" data-parsley-type="email" data-parsley-trigger="change" data-parsley-error-message="valid email required" required="">
					<label for="family-email" class="label required-field">Email Address</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="password" id="family-password" class="input" name="password"
							data-parsley-trigger="focusout"
							data-parsley-minlength="8" data-parsley-minlength-message="password must be 8 or more characters"
							data-parsley-required-message="password is required" required=""
							data-parsley-pattern="/(?=.*\d)(?=.*[a-z])(?=.*[A-Z])/" data-parsley-pattern-message="password does not meet requirements">
					<label for="family-password" class="label required-field">Password</label>
				</div>
				<div class="col-sm-12">
					<input type="password" id="family-confirm-password" class="input" name="confirmPassword"
							data-parsley-trigger="focusout"
							data-parsley-minlength="8" data-parsley-minlength-message="password must be 8 or more characters"
							data-parsley-required-message="password is required" required=""
							data-parsley-pattern="/(?=.*\d)(?=.*[a-z])(?=.*[A-Z])/" data-parsley-pattern-message="password does not meet requirements"
							data-parsley-equalto="#family-password" data-parsley-equalto-message="passwords must match">
					<label for="family-confirm-password" class="label required-field">Confirm Password</label>
				</div>
			</div>
			<div>
				<p class="form-field-note">Note: passwords must be at least 8 characters long, contain at least one number, one uppercase letter, and one lowercase letter.</p>
			</div>
		</section>

		<section class="family-contact-1-section">
			<div class="form-section-header">Contact 1</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="contact1-first-name" class="input" name="contact1FirstName" data-parsley-error-message="required" required="">
					<label for="contact1-first-name" class="label required-field">First Name</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="contact1-last-name" class="input" name="contact1LastName" data-parsley-error-message="required" required="">
					<label for="contact1-last-name" class="label required-field">Last Name</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="email" id="contact1-email" class="input" name="contact1Email" data-parsley-type="email" data-parsley-trigger="change" data-parsley-error-message="valid email required" required="">
					<label for="contact1-email" class="label required-field">Email Address</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="contact1-mobile" class="input" name="contact1Mobile" data-parsley-required-message="required" required=""
						data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
					<label for="contact1-mobile" class="label required-field">Mobile Phone</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<select id="contact1-preferred-communication-method" class="input" name="contact1PreferredCommunicationMethod" data-parsley-errors-messages-disabled="true" required="">
						<option value=""></option>
						<option value="email">email</option>
						<option value="home phone">home</option>
						<option value="mobile phone">mobile</option>
					</select>
					<label for="contact1-preferred-communication-method" class="label required-field">Preferred Contact Method</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-24">
					<span id="contact1-gender-error-container" class="error-container">
						<span class="gender-select-text required-field">Gender</span> &nbsp;&nbsp;
						{{#each genders}}
							<label class="radio-inline"><input type="radio" name="contact1Gender" value="{{_id}}" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#contact1-gender-error-container" required="">{{gender}}</label>
						{{/each}}
					</span>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<div class="row form-group">
						<div class="col-sm-24">
							<span class="required-field">Race/Ethnicity (check all that apply)</span>
						</div>
					</div>
					<div id="contact1-race-error-container" class="row form-group error-container">
						{{#each races}}
						<div class="col-sm-12 form-group">
							<label class="checkbox-inline">
								<input type="checkbox" name="contact1Race[]" value="{{_id}}" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#contact1-race-error-container" required="">{{race}}
							</label>
						</div>
						{{/each}}
					</div>
				</div>
				<div class="col-sm-12">
					<div class="form-group">
						<input type="text" id="contact1-occupation" class="input" name="contact1Occupation">
						<label for="contact1-occupation" class="label">Occupation</label>
					</div>
					<div class="form-group">
						<input type="text" class="input" id="contact1-date-of-birth" name="contact1DateOfBirth" data-parsley-required-message="required" required="" placeholder="MM/DD/YYYY"
							data-parsley-pattern="/^((0?[1-9])|(1[0-2]))\/((0?[1-9])|([1-2]?[0-9])|(3[0-1]))\/\d{4}$/" data-parsley-pattern-message="Please use MM/DD/YYYY format">
						<label for="contact1-date-of-birth" class="label required-field">Date of Birth</label>
					</div>
				</div>
			</div>
            <div class="row form-group">
                <div class="col-sm-12">
					<select id="relationship-status" class="input" name="relationshipStatus" data-parsley-errors-messages-disabled="true" required="true">
						<option value=""></option>
						<option value="Single">single</option>
						<option value="Partnered">partnered</option>
						<option value="Unknown/Prefers Not To Answer">do not wish to answer</option>
					</select>
					<label for="relationship-status" class="label required-field">Are you single or partnered?</label>
				</div>
            </div>
            <div class="row form-group">
                <div class="col-sm-12">
					<select id="contact1-lgbtq-community" class="input" name="contact1LGBTQCommunity" data-parsley-errors-messages-disabled="true">
						<option value=""></option>
						<option value="Yes">yes</option>
						<option value="No">no</option>
						<option value="Unknown/Prefers Not To Answer">do not wish to answer</option>
					</select>
					<label for="contact1-lgbtq-community" class="label">Do you identify as part of the LGBTQ+ community?</label>
				</div>
                <div class="col-sm-12">
					<textarea class="input" id="contact1-lgbtq-community-comments" name="contact1LGBTQCommunityComments" rows="2"></textarea>
					<label for="contact1-lgbtq-community-comments" class="label">Identity details you wish to share</label>
				</div>
            </div>
		</section>

		<section class="family-contact-2-section">
			<div class="form-section-header">Contact 2</div>

			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="contact2-first-name" class="input" name="contact2FirstName">
					<label for="contact2-first-name" class="label">First Name</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="contact2-last-name" class="input" name="contact2LastName">
					<label for="contact2-last-name" class="label">Last Name</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="email" id="contact2-email" class="input" name="contact2Email" data-parsley-type="email" data-parsley-trigger="change" data-parsley-required="false" data-parsley-error-message="valid email required">
					<label for="contact2-email" class="label">Email Address</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="contact2-phone" class="input" name="contact2Mobile" data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
					<label for="contact2-phone" class="label">Mobile Phone</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<select id="contact2-preferred-communication-method" class="input" name="contact2PreferredCommunicationMethod">
						<option value=""></option>
						<option value="email">email</option>
						<option value="home phone">home</option>
						<option value="mobile phone">mobile</option>
					</select>
					<label for="contact2-preferred-communication-method" class="label">Preferred Contact Method</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-24">
					<span class="gender-select-text">Gender:</span> &nbsp;&nbsp;
					{{#each genders}}
						<label class="radio-inline"><input type="radio" name="contact2Gender" value="{{_id}}">{{gender}}</label>
					{{/each}}
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<div class="row form-group">
						<div class="col-sm-24">
							<span>Race/Ethnicity (check all that apply)</span>
						</div>
					</div>
					<div class="row form-group">
						{{#each races}}
						<div class="col-sm-12 form-group">
							<label class="checkbox-inline">
								<input type="checkbox" name="contact2Race[]" value="{{_id}}">{{race}}
							</label>
						</div>
						{{/each}}
					</div>
				</div>
				<div class="col-sm-12">
					<div class="form-group">
						<input type="text" id="contact2-occupation" class="input" name="contact2Occupation">
						<label for="contact2-occupation" class="label">Occupation</label>
					</div>
					<div class="form-group">
						<input type="text" id="contact2-date-of-birth" class="input" name="contact2DateOfBirth" placeholder="MM/DD/YYYY"
							data-parsley-pattern="/^((0?[1-9])|(1[0-2]))\/((0?[1-9])|([1-2]?[0-9])|(3[0-1]))\/\d{4}$/" data-parsley-pattern-message="Please use MM/DD/YYYY format">
						<label for="contact2-date-of-birth" class="label">Date of Birth</label>
					</div>
				</div>
			</div>
            <div class="row form-group">
                <div class="col-sm-12">
					<select id="contact2-lgbtq-community" class="input" name="contact2LGBTQCommunity" data-parsley-errors-messages-disabled="true">
						<option value=""></option>
						<option value="Yes">yes</option>
						<option value="No">no</option>
						<option value="Unknown/Prefers Not To Answer">do not wish to answer</option>
					</select>
					<label for="contact2-lgbtq-community" class="label">Do you identify as part of the LGBTQ+ community?</label>
				</div>
                <div class="col-sm-12">
					<textarea class="input" id="contact2-lgbtq-community-comments" name="contact2LGBTQCommunityComments" rows="2"></textarea>
					<label for="contact2-lgbtq-community-comments" class="label">Identity details you wish to share</label>
				</div>
            </div>
		</section>

		<section class="family-home-address-section">
			<div class="form-section-header">Home Address</div>

			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="family-street-1" class="input" name="street1" data-parsley-error-message="required" required="">
					<label for="family-street-1" class="label required-field">Street 1</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="family-street-2" class="input" name="street2">
					<label for="family-street-2" class="label">Street 2</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12 form-group city-container">
					<select class="input city" name="city" data-parsley-trigger="change" data-parsley-errors-messages-disabled="true" required="">
						<option value=""></option>
						{{#each citiesAndTowns}}
						<option value="{{ _id }}">{{ cityOrTown }}</option>
						{{/each}}
					</select>
					<label for="city" class="label required-field">City</label>
				</div>
				<div class="col-sm-12 form-group non-ma-city-container hidden">
					<input type="text" id="family-non-ma-city" class="input non-ma-city" name="nonMACity" data-parsley-error-message="required" data-parsley-required="false">
					<label for="family-non-ma-city" class="label required-field">City</label>
				</div>
				<div class="col-sm-12 form-group">
					<label class="checkbox-inline">
						<input type="checkbox" class="is-not-ma-city-checkbox" name="isNotMACity">I live outside MA
					</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<select id="family-state" class="input state-select" name="state">
					{{#each states}}
						{{#if defaultSelection}}
							<option value="{{_id}}" selected="selected">{{state}}</option>
						{{else}}
							<option value="{{_id}}">{{state}}</option>
						{{/if}}
					{{/each}}
					</select>
					<label for="state" class="label required-field">State</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="number" id="zip-code" class="input" name="zipCode" data-parsley-required-message="required" required=""
						 data-parsley-pattern="/^[0-9]{5}$/" data-parsley-pattern-message="Please use xxxxx format">
					<label for="zip-code" class="label required-field">Zip Code</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="home-phone" class="input" name="homePhone" data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
					<label for="home-phone" class="label">Home Phone</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-24">
					<label class="label marg-bottom-xs-5 required-field">Where are you in the adoption process?</label>
				</div>
			</div>
			<div class="row form-group">
				<div id="adoption-process-error-container" class="col-sm-16 error-container">
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="gathering-information-checkbox" class="adoption-preferences-trigger" name="processProgression" value="gatheringInformation" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#adoption-process-error-container" required="">Gathering Information
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="looking-for-agency-checkbox" class="adoption-preferences-trigger" name="processProgression" value="lookingForAgency" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#adoption-process-error-container" required="">Looking for an agency
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="working-with-agency-checkbox" class="adoption-preferences-trigger" name="processProgression" value="workingWithAgency" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#adoption-process-error-container" required="">Working with an agency
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="mapp-training-completed-checkbox" class="adoption-preferences-trigger" name="processProgression" value="MAPPTrainingCompleted" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#adoption-process-error-container" required="">MAPP training completed
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="homestudy-completed-checkbox" class="adoption-preferences-trigger" name="processProgression" value="homestudyCompleted" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#adoption-process-error-container" required="">Homestudy completed
						</label>
					</div>
				</div>
			</div>
		</section>
		<!-- This will end part 3 -->

		<section class="family-homestudy-details-section hidden">
			<div class="form-section-header">Homestudy Details</div>

			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="homestudy-date-complete" class="input" name="homestudyDateComplete" data-parsley-required="false" data-parsley-required-message="required" placeholder="MM/DD/YYYY"
						data-parsley-pattern="/^((0?[1-9])|(1[0-2]))\/((0?[1-9])|([1-2]?[0-9])|(3[0-1]))\/\d{4}$/" data-parsley-pattern-message="Please use MM/DD/YYYY format">
					<label for="homestudy-date-complete" class="label required-field">Date Homestudy Completed/Updated</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="social-worker-name" class="input" name="socialWorkerName" data-parsley-required="false" data-parsley-error-message="required">
					<label for="social-worker-name" class="label required-field">Social Worker Name</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="social-worker-agency" class="input" name="socialWorkerAgency" data-parsley-required="false" data-parsley-error-message="required">
					<label for="social-worker-agency" class="label required-field">Social Worker Agency</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="social-worker-phone" class="input" name="socialWorkerPhone" data-parsley-required="false" data-parsley-required-message="required"
						data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
					<label for="social-worker-phone" class="label required-field">Social Worker Phone Number</label>
				</div>
				<div class="col-sm-12">
					<input type="email" id="family-form-social-worker-email" class="input social-worker-email" name="socialWorkerEmail" data-parsley-type="email" data-parsley-trigger="change" data-parsley-required="false" data-parsley-error-message="valid email required">
					<label for="family-form-social-worker-email" class="label required-field">Social Worker Email Address</label>
				</div>
			</div>

			<section class="subheading family-submit-your-homestudy-section">
				<div class="row form-group">
					<div class="col-sm-12"><strong>Submit Your Homestudy</strong></div>
				</div>
				<div class="row form-group">
					<div class="col-sm-24">To complete your registration as a Massachusetts homestudied family, you must submit your homestudy to us via one of the following methods (choose one):</div>
				</div>
				{{!-- <div class="row form-group">
					<div class="col-sm-12">Upload PDF or Microsoft Word<sup>&reg;</sup> format:</div>
					<div class="col-sm-12">
						<div class="button button--medium button--teal file-upload-button">
						    <span>Upload</span>
						    <input id="upload-button" type="file" class="file-upload" id="homestudyFile_upload" name="homestudyFile_upload">
						</div>
						<span class="homestudy-file-text"></span>
					</div>
				</div> --}}
				<div class="row form-group">
					<div class="col-sm-12">Email to lizj@mareinc.org:</div>
					<div class="col-sm-12">
						<div class="button button--medium button--teal"><a href="mailto:lizj@mareinc.org">Send as email</a></div>
					</div>
				</div>
				<div class="row form-group">
					<div class="col-sm-12">Fax to (617) 244-3983, Attention: Homestudy Registration</div>
				</div>
				<div class="row form-group">
					<div class="col-sm-12">
						<div class="indented-text">
							Mail to:
							<div class="indent-text">
								Massachusetts Adoption Resource Exchange<br />
								19 Needham Street<br />
								Suite 206<br />
								Newton, MA 02461
							</div>
						</div>
					</div>
				</div>
			</section>
		</section>

		<section class="family-details-section">
			<div class="form-section-header">Family Details</div>

			<div class="row form-group children-in-home">
				<div class="col-sm-12">
					<label for="children-in-home" class="label marg-bottom-xs-5">Total number of children currently living at home</label>
				</div>
				<div class="col-sm-12">
					<select id="children-in-home" name="childrenInHome" class="input form__number-select">
						<option value=""></option>
						<option value="0">0</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
						<option value="4">4</option>
						<option value="5">5</option>
						<option value="6">6</option>
						<option value="7">7</option>
						<option value="8">8+</option>
					</select>
				</div>
			</div>
			<section class="children-in-home-details"></section>

			<hr class="registration-divider">

			<div class="row form-group">
				<div class="col-sm-12">
					<label for="other-adults-in-home" class="label marg-bottom-xs-5">Number of OTHER adults living in home (not listed above)</label>
				</div>
				<div class="col-sm-12">
					<select id="other-adults-in-home" name="otherAdultsInHome" class="input form__number-select">
						<option value=""></option>
						<option value="0">0</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
						<option value="4">4</option>
						<option value="5">5</option>
						<option value="6">6</option>
						<option value="7">7</option>
						<option value="8">8</option>
						<option value="9">9</option>
						<option value="10">10+</option>
					</select>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-24">
					<label class="checkbox-inline">
						<input type="checkbox" name="havePets" value="yes">Have pets in the home
					</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<label for="primary-language-in-home" class="label marg-bottom-xs-5">Primary language spoken at home</label>
				</div>
				<div class="col-sm-12">
					<select name="primaryLanguageInHome" class="input form__number-select">
						<option value=""></option>
						{{#each languages}}
							<option value="{{_id}}">{{language}}</option>
						{{/each}}
					</select>
				</div>
			</div>
			<div class="row form-group">
