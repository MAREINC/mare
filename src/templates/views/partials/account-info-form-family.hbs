<div class="registration-form form--family-registration">
	<section class="family-account-setup-section">
		<div class="row form-group">
			<div class="col-sm-12">
				<input type="email" id="email" class="input" name="email" data-field-name="email" value="{{user.email}}" pattern="^[a-z0-9._%+'-]+@[a-z0-9.-]+\.[a-z]{2,4}$">
				<label for="email" class="label">Email Address</label>
			</div>
		</div>
	</section>

	<section class="family-contact-1-section">
		<div class="form-section-header">Contact 1</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<input type="text" id="contact1-first-name" class="input" name="contact1FirstName" data-field-name="contact1.name.first" value="{{user.contact1.name.first}}">
				<label for="contact1-first-name" class="label">First Name</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="contact1-last-name" class="input" name="contact1LastName" data-field-name="contact1.name.last" value="{{user.contact1.name.last}}">
				<label for="contact1-last-name" class="label">Last Name</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<input type="email" id="contact1-email" class="input" name="contact1Email" data-field-name="contact1.email" value="{{user.contact1.email}}" pattern="^[a-z0-9._%+'-]+@[a-z0-9.-]+\.[a-z]{2,4}$">
				<label for="contact1-email" class="label">Email Address</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="contact1-mobile" class="input" name="contact1Mobile" data-field-name="contact1.phone.mobile" value="{{user.contact1.phone.mobile}}"
					data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
				<label for="contact1-mobile" class="label">Mobile Phone</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<select id="contact1-preferred-communication-method" class="input" name="contact1PreferredCommunicationMethod" data-field-name="contact1.preferredCommunicationMethod">
					<option value=""></option>
					<option {{#ifeq user.contact1.preferredCommunicationMethod 'email'}}selected{{/ifeq}} value="email">email</option>
					<option {{#ifeq user.contact1.preferredCommunicationMethod 'home phone'}}selected{{/ifeq}} value="home phone">home</option>
					<option {{#ifeq user.contact1.preferredCommunicationMethod 'mobile phone'}}selected{{/ifeq}} value="mobile phone">mobile</option>
				</select>
				<label for="contact1-preferred-communication-method" class="label">Preferred Contact Method</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-24">
				<span id="contact1-gender-error-container" class="error-container">
					<span class="gender-select-text">Gender:</span> &nbsp;&nbsp;
					{{#each genders}}
						<label class="radio-inline"><input id="contact1-gender" type="radio" name="contact1Gender" data-field-name="contact1.gender" value="{{_id}}" {{#ifeq ../user.contact1.gender _id}}checked="checked"{{/ifeq}}>{{gender}}</label>
					{{/each}}
				</span>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<div class="row form-group">
					<div class="col-sm-24">
						<span class="required-field">Race/Ethnicity (check all that apply)</span>
					</div>
				</div>
				<div id="contact1-race-error-container" class="row form-group error-container">
					{{#each races}}
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input id="contact1-race" type="checkbox" name="contact1Race" data-field-name="contact1.race" data-field-type="select-multiple" value="{{_id}}" {{#ifincludes ../user.contact1.race _id}}checked="checked"{{/ifincludes}}>{{race}}
						</label>
					</div>
					{{/each}}
				</div>
			</div>
			<div class="col-sm-12">
				<div class="form-group">
					<input type="text" id="contact1-occupation" class="input" name="contact1Occupation" data-field-name="contact1.occupation" value="{{user.contact1.occupation}}">
					<label for="contact1-occupation" class="label">Occupation</label>
				</div>
			</div>
		</div>
	</section>

	<section class="family-contact-2-section">
		<div class="form-section-header">Contact 2</div>

		<div class="row form-group">
			<div class="col-sm-12">
				<input type="text" id="contact2-first-name" class="input" name="contact2FirstName" data-field-name="contact2.name.first" value="{{user.contact2.name.first}}">
				<label for="contact2-first-name" class="label">First Name</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="contact2-last-name" class="input" name="contact2LastName" data-field-name="contact2.name.last" value="{{user.contact2.name.last}}">
				<label for="contact2-last-name" class="label">Last Name</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<input type="email" id="contact2-email" class="input" name="contact2Email" data-field-name="contact2.email" value="{{user.contact2.email}}" pattern="^[a-z0-9._%+'-]+@[a-z0-9.-]+\.[a-z]{2,4}$">
				<label for="contact2-email" class="label">Email Address</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="contact2-phone" class="input" name="contact2Mobile" data-field-name="contact2.phone.mobile" value="{{user.contact2.phone.mobile}}"
					data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
				<label for="contact2-phone" class="label">Mobile Phone</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<select id="contact2-preferred-communication-method" class="input" name="contact2PreferredCommunicationMethod" data-field-name="contact2.preferredCommunicationMethod">
					<option value=""></option>
					<option {{#ifeq user.contact2.preferredCommunicationMethod 'email'}}selected{{/ifeq}} value="email">email</option>
					<option {{#ifeq user.contact2.preferredCommunicationMethod 'home phone'}}selected{{/ifeq}} value="home phone">home</option>
					<option {{#ifeq user.contact2.preferredCommunicationMethod 'mobile phone'}}selected{{/ifeq}} value="mobile phone">mobile</option>
				</select>
				<label for="contact2-preferred-communication-method" class="label">Preferred Contact Method</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-24">
				<span class="gender-select-text">Gender:</span> &nbsp;&nbsp;
				{{#each genders}}
					<label class="radio-inline"><input type="radio" name="contact2Gender" data-field-name="contact2.gender" value="{{_id}}" {{#ifeq ../user.contact2.gender _id}}checked="checked"{{/ifeq}}>{{gender}}</label>
				{{/each}}
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<div class="row form-group">
					<div class="col-sm-24">
						<span>Race/Ethnicity (check all that apply)</span>
					</div>
				</div>
				<div class="row form-group">
					{{#each races}}
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" name="contact2Race" data-field-name="contact2.race" data-field-type="select-multiple" value="{{_id}}" {{#ifincludes ../user.contact2.race _id}}checked="checked"{{/ifincludes}}>{{race}}
						</label>
					</div>
					{{/each}}
				</div>
			</div>
			<div class="col-sm-12">
				<div class="form-group">
					<input type="text" id="contact2-occupation" class="input" name="contact2Occupation" data-field-name="contact2.occupation" value="{{user.contact2.occupation}}">
					<label for="contact2-occupation" class="label">Occupation</label>
				</div>
			</div>
		</div>
	</section>

	<section class="family-home-address-section">
		<div class="form-section-header">Home Address</div>

		<div class="row form-group">
			<div class="col-sm-12">
				<input type="text" id="street-1" class="input" name="street1" data-field-name="address.street1" value="{{user.address.street1}}">
				<label for="street-1" class="label">Street 1</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="street-2" class="input" name="street2" data-field-name="address.street2" value="{{user.address.street2}}">
				<label for="street-2" class="label">Street 2</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12 form-group city-container {{#if user.address.isOutsideMassachusetts}}hidden{{/if}}">
				<select id="city" class="input" name="city" data-field-name="address.city">
					<option value=""></option>
					{{#each citiesAndTowns}}
					<option value="{{_id}}" {{#ifeq ../user.address.city _id}}selected="selected"{{/ifeq}}>{{cityOrTown}}</option>
					{{/each}}
				</select>
				<label for="city" class="label">City</label>
			</div>
			<div class="col-sm-12 form-group non-ma-city-container {{#unless user.address.isOutsideMassachusetts}}hidden{{/unless}}">
				<input type="text" id="non-ma-city" class="input" name="nonMACity" data-field-name="address.cityText" value="{{user.address.cityText}}">
				<label for="non-ma-city" class="label">City</label>

				<select id="family-state" class="input state-select" name="state" data-field-name="address.state">
				{{#each states}}
					{{#if defaultSelection}}
						<option value="{{_id}}" selected="selected">{{state}}</option>
					{{else}}
						<option value="{{_id}}">{{state}}</option>
					{{/if}}
				{{/each}}
				</select>
				<label for="state" class="label">State</label>
			</div>
			<div class="col-sm-12 form-group">
				<label class="checkbox-inline">
					<input type="checkbox" id="is-not-ma-city-checkbox" name="isNotMACity" data-field-name="address.isOutsideMassachusetts" value="{{user.address.isOutsideMassachusetts}}" {{#if user.address.isOutsideMassachusetts}}checked="checked"{{/if}}>I live outside MA
				</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">

			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<input type="number" id="zip-code" class="input" name="zipCode" data-field-name="address.zipCode" value="{{user.address.zipCode}}"
					data-parsley-pattern="/^[0-9]{5}$/" data-parsley-pattern-message="Please use xxxxx format">
				<label for="zip-code" class="label">Zip Code</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="home-phone" class="input" name="homePhone" data-field-name="homePhone" value="{{user.homePhone}}"
					data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
				<label for="home-phone" class="label">Home Phone</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-24">
				<label class="label marg-bottom-xs-5">Where are you in the adoption process?</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-16">
				<div class="row form-group">
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="gathering-information-checkbox" class="adoption-preferences-trigger" name="processProgression" data-field-name="gatheringInformation.started" value="{{user.stages.gatheringInformation.started}}" {{#if user.stages.gatheringInformation.started}}checked="checked"{{/if}}>Gathering Information
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="looking-for-agency-checkbox" class="adoption-preferences-trigger" name="processProgression" data-field-name="stages.lookingForAgency.started" value="{{user.stages.lookingForAgency.started}}" {{#if user.stages.lookingForAgency.started}}checked="checked"{{/if}}>Looking for an agency
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="working-with-agency-checkbox" class="adoption-preferences-trigger" name="processProgression" data-field-name="stages.workingWithAgency.started"  value="{{user.stages.workingWithAgency.started}}" {{#if user.stages.workingWithAgency.started}}checked="checked"{{/if}}>Working with an agency
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="mapp-training-completed-checkbox" class="adoption-preferences-trigger" name="processProgression" data-field-name="stages.MAPPTrainingCompleted.completed" value="{{user.stages.MAPPTrainingCompleted.completed}}" {{#if user.stages.MAPPTrainingCompleted.completed}}checked="checked"{{/if}}>MAPP training completed
						</label>
					</div>
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" id="homestudy-completed-checkbox" class="adoption-preferences-trigger" name="processProgression" data-field-name="homestudy.completed" value="{{user.homestudy.completed}}" {{#if user.homestudy.completed}}checked="checked"{{/if}}>Homestudy completed
						</label>
					</div>
				</div>
			</div>
		</div>
	</section>

	<section class="family-homestudy-details-section {{#unless user.homestudy.completed}}hidden{{/unless}}">
		<div class="form-section-header">Homestudy Details</div>

		<div class="row form-group">
			<div class="col-sm-12">
				<input type="text" id="homestudy-date-complete" class="input" placeholder="MM/DD/YYYY" value="{{#if homestudyCompletedDate}}{{homestudyCompletedDate}}{{/if}}" disabled="true">
				<label for="homestudy-date-complete" class="label">Date Homestudy Completed/Updated</label>
			</div>
			<div class="col-sm-12">
				<input type="text" id="social-worker-name" class="input" value="{{familySocialWorker}}" disabled="true">
				<label for="social-worker-name" class="label">Social Worker Name</label>
			</div>
		</div>

		<section class="subheading family-submit-your-homestudy-section">
			<div class="row form-group">
				<div class="col-sm-12"><strong>Submit Your Homestudy</strong></div>
			</div>
			<div class="row form-group">
				<div class="col-sm-24">To make updates to this information, or to send us your approved homestudy for matching (MA families only), please email us:</div>
			</div>		
			<div class="row form-group">
				<div class="col-sm-12">
					<div class="button button--medium button--teal"><a href="mailto:ricardof@mareinc.org">Send as email</a></div>
				</div>
			</div>
		</section>
	</section>

	<section class="family-details-section">
		<div class="form-section-header">Family Details</div>

		<div class="row form-group children-in-home">
			<div class="col-sm-12">
				<label for="children-in-home" class="label marg-bottom-xs-5">Total number of children currently living at home</label>
			</div>
			<div class="col-sm-12">
				<select id="children-in-home" name="childrenInHome" class="input form__number-select" data-field-name="numberOfChildren">
					<option {{#ifeq user.numberOfChildren '0'}}selected{{/ifeq}}>0</option>
					<option {{#ifeq user.numberOfChildren '1'}}selected{{/ifeq}}>1</option>
					<option {{#ifeq user.numberOfChildren '2'}}selected{{/ifeq}}>2</option>
					<option {{#ifeq user.numberOfChildren '3'}}selected{{/ifeq}}>3</option>
					<option {{#ifeq user.numberOfChildren '4'}}selected{{/ifeq}}>4</option>
					<option {{#ifeq user.numberOfChildren '5'}}selected{{/ifeq}}>5</option>
					<option {{#ifeq user.numberOfChildren '6'}}selected{{/ifeq}}>6</option>
					<option {{#ifeq user.numberOfChildren '7'}}selected{{/ifeq}}>7</option>
					<option {{#ifeq user.numberOfChildren '8+'}}selected{{/ifeq}}>8+</option>
				</select>
			</div>
		</div>
		<section class="children-in-home-details"></section>

		<hr class="registration-divider">

		<div class="row form-group">
			<div class="col-sm-12">
				<label for="other-adults-in-home" class="label marg-bottom-xs-5">Number of other adults living in home</label>
			</div>
			<div class="col-sm-12">
				<select id="other-adults-in-home" name="otherAdultsInHome" class="input form__number-select" data-field-name="otherAdultsInHome.number">
					<option {{#ifeq user.otherAdultsInHome.number '0'}}selected{{/ifeq}}>0</option>
					<option {{#ifeq user.otherAdultsInHome.number '1'}}selected{{/ifeq}}>1</option>
					<option {{#ifeq user.otherAdultsInHome.number '2'}}selected{{/ifeq}}>2</option>
					<option {{#ifeq user.otherAdultsInHome.number '3'}}selected{{/ifeq}}>3</option>
					<option {{#ifeq user.otherAdultsInHome.number '4'}}selected{{/ifeq}}>4</option>
					<option {{#ifeq user.otherAdultsInHome.number '5'}}selected{{/ifeq}}>5</option>
					<option {{#ifeq user.otherAdultsInHome.number '6'}}selected{{/ifeq}}>6</option>
					<option {{#ifeq user.otherAdultsInHome.number '7'}}selected{{/ifeq}}>7</option>
					<option {{#ifeq user.otherAdultsInHome.number '8'}}selected{{/ifeq}}>8</option>
					<option {{#ifeq user.otherAdultsInHome.number '9'}}selected{{/ifeq}}>9</option>
					<option {{#ifeq user.otherAdultsInHome.number '10+'}}selected{{/ifeq}}>10+</option>
				</select>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-24">
				<label class="checkbox-inline">
					<input type="checkbox" name="havePets" data-field-name="matchingPreferences.havePetsInHome" value="{{user.matchingPreferences.havePetsInHome}}"  {{#if user.matchingPreferences.havePetsInHome}}checked="checked"{{/if}}>Have pets in the home
				</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<label for="primary-language-in-home" class="label marg-bottom-xs-5">Primary language spoken at home</label>
			</div>
			<div class="col-sm-12">
				<select name="primaryLanguageInHome" class="input form__number-select" data-field-name="language">
					<option value=""></option>
					{{#each languages}}
						<option value="{{_id}}" {{#ifeq ../user.language _id}}selected="selected"{{/ifeq}}>{{language}}</option>
					{{/each}}
				</select>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-24">
				<label class="label marg-bottom-xs-5">Other language(s) spoken at home</label>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-16">
				<div class="row">
				{{#each languages}}
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" name="otherLanguagesInHome" data-field-name="otherLanguages" data-field-type="select-multiple" value="{{_id}}"  {{#ifincludes ../user.otherLanguages _id}}checked{{/ifincludes}}>{{language}}
						</label>
					</div>
				{{/each}}
				</div>
			</div>
		</div>
	</section>

	<section class="family-adoption-preferences-section">
		<div class="form-section-header">Adoption Preferences</div>

		<div class="row form-group">
			<div class="col-sm-12">
				<label class="label marg-bottom-xs-5 preferred-gender-label">Gender of children preferred</label>
			</div>
			<div class="col-sm-12">
				<div id="preferred-gender-error-container" class="row error-container">
				{{#each genders}}
					<div class="col-sm-12 form-group">
						<label class="checkbox-inline">
							<input type="checkbox" name="preferredGender" class="preferred-gender" data-field-name="matchingPreferences.gender" data-field-type="select-multiple" value="{{_id}}" {{#ifincludes ../user.matchingPreferences.gender _id}}checked="checked"{{/ifincludes}}>{{gender}}
						</label>
					</div>
				{{/each}}
				</div>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<label class="label marg-bottom-xs-5 legal-status-label">Legal Status</label>
			</div>
			<div class="col-sm-12">
				<div id="preferred-legal-status-error-container" class="row error-container">
				{{#each legalStatuses}}
					<div class="col-sm-12">
						<label class="checkbox-inline">
							<input type="checkbox" name="legalStatus" class="legal-status" data-field-name="matchingPreferences.legalStatus" data-field-type="select-multiple" value="{{_id}}" {{#ifincludes ../user.matchingPreferences.legalStatus _id}}checked="checked"{{/ifincludes}}>{{legalStatus}}
						</label>
					</div>
				{{/each}}
				</div>
			</div>
		</div>
		<div class="row form-group">
			<div class="col-sm-12">
				<label class="label marg-bottom-xs-5 age-range-label">Age Range</label>
			</div>
			<div class="col-sm-12">
				<div class="row">
					<div class="col-xs-8">
						<select name="ageRangeFrom" class="input form__number-select inline age-range-from" data-field-name="matchingPreferences.adoptionAges.from">
							{{!-- TODO: Find ...adoptionAges FROM value --}}
							<option value=""></option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 0}}selected="selected"{{/ifeq}} value="0">0</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 1}}selected="selected"{{/ifeq}} value="1">1</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 2}}selected="selected"{{/ifeq}} value="2">2</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 3}}selected="selected"{{/ifeq}} value="3">3</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 4}}selected="selected"{{/ifeq}} value="4">4</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 5}}selected="selected"{{/ifeq}} value="5">5</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 6}}selected="selected"{{/ifeq}} value="6">6</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 7}}selected="selected"{{/ifeq}} value="7">7</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 8}}selected="selected"{{/ifeq}} value="8">8</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 9}}selected="selected"{{/ifeq}} value="9">9</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 10}}selected="selected"{{/ifeq}} value="10">10</option>
							<option {{#ifeq user.matchingPreferences.adoptionAges.from 11}}selected="selected"{{/ifeq}} value="11">11</option>
